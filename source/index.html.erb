              <a type="button" class="" data-toggle="tooltip" data-placement="bottom" title="Figma">
                <i class="fab fa-figma"></i>
              </a>
            </div><!-- icons-inline -->
          </div> <!-- col-sm -->
        </div> <!-- row -->
      </div><!-- row ???? -->
